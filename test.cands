#   Lexical items
let me:   Li = < { "me'" }   , { "D" }              , [ "me" ]   >;
let HELP: Li = < { "help'" } , { "V", "=D" }        , [ "HELP" ] >;
let v:    Li = < {}          , { "v", "=V", "=D" }  , [ "v" ]    >;
let she:  Li = < { "she'" }  , { "D" }              , [ "she" ]  >;
let PAST: Li = < {}          , { "T", "=v", "epp" } , [ "PAST" ] >;
let C:    Li = < {}          , { "C", "=T" }        , [ "C" ]    >;